  },
  "homepage": "https://github.com/shoveller/offline-class#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "style-dictionary": "^3.7.1",
    "token-transformer": "^0.0.24"
  }
}
